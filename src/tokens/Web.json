  "borders": {
    "divider": {
      "primary": {
        "$type": "border",
        "$value": {
          "color": "{border.default}",
          "width": "{borderWidth.s}",
          "style": "solid"
        }
      },
      "secondary": {
        "$type": "border",
        "$value": {
          "color": "{border.subtle}",
          "width": "{borderWidth.s}",
          "style": "solid"
        }
      }
    },
    "radio": {
      "outside-hover": {
        "$type": "border",
        "$value": {
          "color": "{border.subtle}",
          "width": "{borderWidth.xl}",
          "style": "solid"
        }
      }
    },
    "input": {
      "primary": {
        "$type": "border",
        "$value": {
          "color": "{border.input}",
          "width": "{borderWidth.s}",
          "style": "solid"
        }
      },
      "hover": {
        "$type": "border",
        "$value": {
          "color": "{border.input-strong}",
          "width": "{borderWidth.s}",
          "style": "solid"
        }
      },
      "active": {
        "$type": "border",
        "$value": {
          "color": "{border.active}",
          "width": "{borderWidth.m}",
          "style": "solid"
        }
      },
      "error": {
        "$type": "border",
        "$value": {
          "color": "{border.negative}",
          "width": "{borderWidth.s}",
          "style": "solid"
        }
      }
    },
    "button": {
      "secondary": {
        "$type": "border",
        "$value": {
          "color": "{border.active-subdued}",
          "width": "{borderWidth.m}",
          "style": "solid"
        }
      }
    },
    "menu": {
      "primary": {
        "$type": "border",
        "$value": {
          "color": "{border.subtle}",
          "width": "{borderWidth.s}",
          "style": "solid"
        }
      }
    }
  }
}
